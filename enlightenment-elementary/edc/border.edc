#define BORDER_BUTTON_ADV2(NAME, TYPE, REL1X, REL1Y, OFF1X, OFF1Y, TO1X, TO1Y, REL2X, REL2Y, OFF2X, OFF2Y, TO2X, TO2Y) \
   part { name: NAME"_bg"; mouse_events: 0;              \
      description { state: "default" 0.0;                \
        rel1.relative: 0.0  0.0;                         \
        rel1.offset: -1 -1;                              \
