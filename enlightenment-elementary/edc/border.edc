   }                                                     \
   part { name: NAME"_detail"; mouse_events: 0;          \
      description { state: "default" 0.0;                \
        max: 16 16;                                      \
        rel1.relative: 0.0  0.0;                         \
        rel1.offset: 4    4;                             \
        rel1.to: NAME"_bg";                              \
        rel2.relative: 1.0 1.0;                          \
        rel2.offset: -5 -5;                              \
        rel2.to: NAME"_bg";                              \
        image.normal: "sym_"TYPE"_normal.png";           \
        color: 255 255 255 255;                          \
     }                                                   \
     description { state: "active" 0.0;                  \
	inherit: "default" 0.0;                          \
        color: 255 255 255 190;                          \
      }                                                  \
   }                                                     \
   part { name: NAME; type: RECT;            /* a rect for positioning */ \
      description { state: "default" 0.0;                \
        align: 1.0 0.0;                                  \
        aspect: 1.0 1.0;  aspect_preference: VERTICAL;   \
        rel1.relative: REL1X REL1Y;                      \
        rel1.offset: OFF1X OFF1Y;                        \
        rel1.to_x: TO1X;                                 \
        rel1.to_y: TO1Y;                                 \
        rel2.relative: REL2X REL2Y;                      \
        rel2.offset: OFF2X OFF2Y;                        \
        rel2.to_x: TO2X;                                 \
        rel2.to_y: TO2Y;                                 \
        color: 0 0 0 0;                                  \
      }                                                  \
   }                                                     \
   part { name: "e.event."NAME; type: RECT; /* a rect for window manager actions */ \
      repeat_events: 1;                                  \
       description { state: "default" 0.0;               \
         rel1.to: NAME;                                  \
         rel2.to: NAME;                                  \
         color: 0 0 0 0;                                 \
         }                                               \
      }
