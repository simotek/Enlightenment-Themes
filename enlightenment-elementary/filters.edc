   filters {
      filter {
         // name: "blurhighperf";
         name: "blurhighperf_disabled";
         script {
            padding_set(0)
            blur { 5 }
         }
      }
   }
   filters {
      filter {
         // name: "blurlowperf";
         name: "blurbackground_none2";
         script {
            padding_set(0)
            blur { 10 }
         }
      }
   }